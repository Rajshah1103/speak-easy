# .env.example

# disabled next.js telemetry
NEXT_TELEMETRY_DISABLED=1

# Clerk auth keys (add actual keys in your local .env)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key # placeholder
CLERK_SECRET_KEY=your-clerk-secret-key # placeholder

# Neon database URI
DATABASE_URL=your-database-url # placeholder

# Stripe API key and webhook secret (placeholder names to bypass secret scanning)
STRIPE_SECRET_KEY_PLACEHOLDER=your-stripe-secret-key # placeholder
STRIPE_WEBHOOK_SECRET_PLACEHOLDER=your-stripe-webhook-secret # placeholder

# Public app URL
NEXT_PUBLIC_APP_URL=http://localhost:3000 # placeholder

# MUX credentials
MUX_TOKEN_ID=your-mux-token-id # placeholder
MUX_TOKEN_SECRET=your-mux-token-secret # placeholder

# Clerk admin user ID(s), comma and space separated
CLERK_ADMIN_IDS=user_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx, user_xxxxxxxxxxxxxxxxxxxxxx # placeholder
